            <xsd:appinfo> 
                <ext:label>Contexts And Dependency Injection V1.2 oder höher</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
