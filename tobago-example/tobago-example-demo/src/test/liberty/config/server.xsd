                <xsd:appinfo> 
                    <ext:label>Kerberos-Konfigurationsdatei</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
