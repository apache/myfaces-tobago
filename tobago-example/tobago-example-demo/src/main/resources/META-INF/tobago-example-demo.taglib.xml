<facelet-taglib
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_3.xsd"
    version="2.3">

  <namespace>http://myfaces.apache.org/tobago/example/demo</namespace>
  <short-name>demo</short-name>

  <function>
    <function-name>bootstrapClass</function-name>
    <function-class>org.apache.myfaces.tobago.example.demo.BootstrapUtils</function-class>
    <function-signature>java.lang.String valueOf(java.lang.String)</function-signature>
  </function>

  <function>
    <function-name>logbackLevel</function-name>
    <function-class>org.apache.myfaces.tobago.example.demo.LoggingController</function-class>
    <function-signature>java.lang.String logbackLevel(java.lang.Integer)</function-signature>
  </function>

  <function>
    <function-name>logbackDate</function-name>
    <function-class>org.apache.myfaces.tobago.example.demo.LoggingController</function-class>
    <function-signature>java.util.Date logbackDate(java.lang.Long)</function-signature>
  </function>

</facelet-taglib>
